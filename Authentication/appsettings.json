{
  "ConnectionStrings": {
    "ConnectionStringUsers": "${ConnectionStringUsers}"
  },
  "TokenConfigs": {
    "Secret": "${MySecret}",
    "TokenExpirationInMinutes": 360,
    "Issuer": "${Issuer}"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "${UriElastic}",
    "ElkPassword": "${ElkPassword}",
    "ElkUsername": "${ElkUsername}"
  },
  "ApiBaseUrlUsers": "{BaseUrlApiUsers}",
  "AllowedHosts": "*"
}